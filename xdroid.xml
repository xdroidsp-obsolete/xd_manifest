<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- xdroid AOSP Remote -->
  <remote  name="xdroidsp"
           revision="xd.xii"
           fetch="https://github.com/xdroidsp" />

  <!-- xyzprjkt Remote -->
  <remote  name="xyz-prjkt"
           fetch="https://github.com/xyz-prjkt" />
  
  <!-- Build Repo -->
  <project path="bionic" name="xd_bionic" remote="xdroidsp" />
  <project path="build/make" name="xd_build" remote="xdroidsp">
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="xd_build_soong" remote="xdroidsp" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <!-- Hardware Qualcomm CAF Repo -->
  <project path="hardware/qcom-caf/msm8998/audio" name="hw_audio" revision="xii.msm8998-caf" remote="xyz-prjkt" />
  <project path="hardware/qcom-caf/msm8998/display" name="hw_display" revision="xii.msm8998-caf" remote="xyz-prjkt" />
  <project path="hardware/qcom-caf/msm8998/media" name="hw_media" revision="xii.msm8998-caf" remote="xyz-prjkt" />

  <!-- Device Repo -->
  <project path="device/qcom/sepolicy" name="xd_device_qcom_sepolicy" remote="xdroidsp" />
  <project path="device/qcom/sepolicy-legacy-um" name="xd_device_qcom_sepolicy-legacy-um" remote="xdroidsp" />
  <project path="device/qcom/sepolicy_vndr" name="xd_device_qcom_sepolicy_vndr" remote="xdroidsp" />
  <project path="device/xdroid/sepolicy" name="xd_device_xdroid_sepolicy" remote="xdroidsp" />

  <!-- External Repo -->
  <project path="external/tinycompress" name="xd_external_tinycompress" remote="xdroidsp" />
  <project path="external/tinyxml" name="xd_external_tinyxml" remote="xdroidsp" />
  <project path="external/selinux" name="xd_external_selinux" remote="xdroidsp" />

  <!-- Hardware Repo -->
  <project path="hardware/libhardware" name="xd_hardware_libhardware" remote="xdroidsp" />
  <project path="hardware/qcom-caf/wlan" name="xd_hardware_qcom-caf_wlan" remote="xdroidsp" />

  <!-- Package Repo -->
  <project path="packages/apps/Settings" name="xd_packages_apps_Settings" remote="xdroidsp" />

  <!-- Vendor Repo -->
  <project path="vendor/xdroid" name="xd_vendor_xdroid" remote="xdroidsp" />
  <project path="vendor/xdroidui" name="xd_vendor_xdroidui" remote="xdroidsp" />
 
  <!-- Vendor CAF Repo -->
  <project path="vendor/qcom/opensource/commonsys/dpm" name="xd_vendor_qcom_opensource_commonsys_dpm-commonsys" remote="xdroidsp" />
  <project path="vendor/qcom/opensource/vibrator" name="xd_vendor_qcom_opensource_vibrator" remote="xdroidsp" />
 
</manifest>
